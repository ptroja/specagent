[comment encoding = UTF-8 /]
[module system2ada('http://multirobotsystem/1.0')/]
[import agent_spec/]

[template public system2ada(aSystem : System)]

[comment @main /]
[file (aSystem.name.concat('.adb'), false, 'UTF-8')]
-- with Ada.Text_IO;
[for (anAgent : Agent | agents)]
with [anAgent.name/]_agent;
[/for]

procedure [aSystem.name/]_system is
begin
   null;
end [aSystem.name/]_system;
[/file]

[for (anAgent : Agent | aSystem.agents)]
[agent_spec(anAgent)/]
[/for]
	
[/template]
