[comment encoding = UTF-8 /]
[module project('http://multirobotsystem/1.0')/]

[template public project(anAgent : Agent)]
	
[comment @main /]
[file (anAgent.name+'.gpr', false, 'UTF-8')]
project [anAgent.name/] is

   package Compiler is
      for Default_Switches ("ada") use ("-g", "-O2", "-gnat05");
   end Compiler;

   for Main use ("system_[anAgent.name/].adb");

end [anAgent.name/];

[/file]
	
[/template]
